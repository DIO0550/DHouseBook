{
  "name": "dhousebook",
  "description": "housebook",
  "version": "0.0.1",
  "engines": {
    "node": ">=12.13.0"
  },
  "homepage": ".",
  "main": "dist/main.js",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:main": "tsc -w -p tsconfig.main.json",
    "dev:render": "cross-env NODE_ENV=development webpack",
    "dev:electron": "wait-on ./dist/index.html && cross-env NODE_ENV=\"development\" electron .",
    "prod": "run-p prod:*",
    "prod:main": "tsc -p tsconfig.main.json",
    "prod:render": "cross-env NODE_ENV=\"production\" webpack --config webpack.config.prod.ts",
    "prod:electron": "wait-on ./dist/index.html && cross-env NODE_ENV=\"production\" electron .",
    "build": "electron-builder --win --x64 --dir",
    "build:win": "npm run prod:main && npm run prod:render && electron-builder && mkdirp dist/win-unpacked/resources/bookdata"
  },
  "build": {
    "productName": "家計簿",
    "appId": "dio.housebook.com",
    "directories": {
      "buildResources": "resources",
      "output": "./dist"
    },
    "files": [
      "dist/**/*.js",
      "dist/**/*.css",
      "dist/index.html",
      "./icon/**/*"
    ],
    "win": {
      "target": {
        "target": "dir",
        "arch": [
          "x64"
        ]
      }
    },
    "mac": {
      "target": "dmg"
    }
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.2",
    "dotenv": "^16.0.0",
    "electron-log": "^4.4.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/mini-css-extract-plugin": "^2.3.0",
    "@types/node": "^16.10.2",
    "@types/react": "^17.0.26",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "asar": "^3.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.3.0",
    "dart-sass": "^1.25.0",
    "electron": "^15.1.0",
    "electron-builder": "^22.11.7",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prefer-arrow-functions": "^3.1.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.2",
    "ifdef-loader": "^2.3.0",
    "mini-css-extract-plugin": "^2.3.0",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "prettier-stylelint": "^0.4.2",
    "rimraf": "^3.0.2",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "stylelint": "^14.1.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-scss": "^4.0.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "typescript-plugin-css-modules": "^3.4.0",
    "webpack": "^5.56.0",
    "webpack-cli": "^4.8.0"
  },
  "optionalDependencies": {
    "electron-notarize": "^1.1.1",
    "electron-reload": "^1.5.0",
    "electron-search-devtools": "^1.4.8",
    "wait-on": "^6.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browser": {
    "crypto": false
  }
}
